{
  "name":         "lumen/translator",
  "description": "Universal translator of [string, array, object].",
  "homepage":     "https://github.com/jcarrizalez/translator",
  "type":         "Library",
  "keywords":     ["composer","laravel","translator","traductor","lenguaje","language"],
  "license":      "MIT",
  "authors": [
    {
      "name": "Juan -Carrizalez",
      "email": "sitgem@gmail.com"

    }
  ],
  "extra": {
    "branch-alias": {
      "dev-master": "2.0"
    }
  },
  "require":      {
    "php": ">= 5.3"
  },
  "require-dev" :   {
    "phpunit/phpunit": "^4.8.35||^5.7||^6.0" 
  },
  "suggest": {
    "ext-intl": "PHP Universal translator of [string, array, object]"
  },
  "autoload": {
    "psr-4": {
      "Translator\\": "src/Translator"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Translator\\": "tests/Translator"
    }
  }
}
